<p id="notice"><%= notice %></p>

<h1>
  <%= t 'collection_template.title' %>
</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collection_templates.each do |collection_template| %>
      <tr>
        <td><%= collection_template.to_s %></td>
        <td><%= link_to t('crud.show'), collection_template %></td>
        <td><%= link_to t('crud.destroy'), collection_template, method: :delete, data: { confirm: t('crud.confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
