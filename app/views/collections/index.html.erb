<h1>Collections</h1>
<div class="container-fluid">
  <% @collections.each do |collection| %>
  <div class="row collection-item">
    <div class="col">
      <%= link_to collection.name, collection %>
    </div>
    <div class="col"><%= collection.description %></div>
  </div>
  <%= collection.to_json  %>
  <% end %>
</div>
